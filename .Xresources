
! big meme
! stolen david colors LUL
urxvt*background: #151515
urxvt*foreground: #eaeaea
! black
URxvt.color0:       #121212
URxvt.color8:       #555555
! red
URxvt.color1:       #fa2573
URxvt.color9:       #f6669d
! green
! URxvt.color2:       #98e123
URxvt.color2:       #9cff00
URxvt.color10:      #b1e05f
! yellow
URxvt.color3:       #dfd460
URxvt.color11:      #fff26d
! blue
URxvt.color4:       #1080d0
URxvt.color12:      #00afff
! magenta
URxvt.color5:       #8700ff
URxvt.color13:      #af87ff
! cyan
URxvt.color6:       #43a8d0
URxvt.color14:      #51ceff
! white
URxvt.color7:       #bbbbbb
URxvt.color15: #ffffff

! stolen david font LUL
URxvt.font:xft:Hack:pixelsize=14

! some more LUL

!URxvt.boldFont:     xft:Hack:pixelsize=11:weight=bold
!URxvt.italicFont: xft:Hack:pixelsize=11:slant=italic

! doesnt scroll down everytime output happens
URxvt*scrollTtyOutput: false
! disable scrollbar
URxvt.scrollBar: false
! makes the scroll buffer long
URxvt.saveLines: 10000
! scroll with buffer so u dont get lost
URxvt.scrollWithBuffer: true
! the bottom scroll on output thing
URxvt.scrollTtyKeypress: true
